{
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    "vim.leader": " ",
    "vim.normalModeKeyBindingsNonRecursive": [
        // VIM remaps
        // VSCode commands
        { "before": [ "leader", "d" ], "commands": [ "editor.action.revealDefinition" ] },
        { "before": [ "leader", "/" ], "commands": [ "editor.action.commentLine" ] },
        { "before": [ "leader", "." ], "commands": [ "editor.action.quickFix" ] },
        { "before": [ "leader", "b" ], "commands": [ "workbench.action.navigateBack" ] },
        { "before": [ "leader", "m" ], "commands": [ "workbench.action.toggleSidebarVisibility" ] },
        { "before": [ "leader", "leader", "f" ], "commands": [ "editor.action.formatDocument" ] },
        { "before": [ "leader", "r" ], "commands": [ "references-view.findReferences" ] },
        { "before": [ "leader", "t", "l" ], "commands": [ "workbench.action.nextEditor" ] },
        { "before": [ "leader", "t", "h" ], "commands": ["workbench.action.previousEditor" ] },
        // Windows
        { "before": [ "leader", "l" ], "commands": [ "workbench.action.focusRightGroup" ] },
        { "before": [ "leader", "h" ], "commands": [ "workbench.action.focusLeftGroup" ] },
        { "before": [ "leader", "j" ], "commands": [ "workbench.action.focusSecondEditorGroup" ] },
        { "before": [ "leader", "k" ], "commands": [ "workbench.action.focusFirstEditorGroup" ] },
        { "before": [ "leader", "q" ], "commands": [ "workbench.action.experimental.quickTextSearch" ] },
        { "before": [ "leader", "right" ], "commands": [ "workbench.action.splitEditorToRightGroup" ] },
        { "before": [ "leader", "left" ], "commands": [ "workbench.action.splitEditorLeft" ] },
        { "before": [ "leader", "tab" ], "commands": [ "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup" ] },
        { "before": [ "leader", "up" ], "commands": [ "workbench.action.toggleMaximizeEditorGroup" ] },

        // VIM EasyMotion
        { "before": [ "s" ], "after": [ "leader", "leader", "s" ] },
        { "before": [ "f" ], "after": ["leader", "leader", "f"] },
        { "before": [ "F" ], "after": ["leader", "leader", "F"] },

        // Other 
        { "before": [ "leader", "," ], "after": [ "g", "h" ] },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        { "before": [ "leader", "/" ], "commands": [ "editor.action.commentLine" ] },
        // VIM EasyMotion
        { "before": [ "s" ], "after": [ "leader", "leader", "s" ] },
        { "before": [ "f" ], "after": ["leader", "leader", "f"] },
        { "before": [ "F" ], "after": ["leader", "leader", "F"] },
        { "before": [ "leader", "leader", "f" ], "commands": [ "editor.action.formatSelection" ]}
    ],
