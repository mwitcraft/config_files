{
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "newUntitledFile",
    "vim.leader": " ",
    "vim.normalModeKeyBindingsNonRecursive": [
        // VIM remaps
        {
            "before": [
                "leader",
                "z"
            ],
            "after": [
                "z",
                "z"
            ]
        },
        // Scroll down
        {
            "before": [
                "J"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll down
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "K"
            ],
            "after": [
                "<C-y>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-y>"
            ]
        },
        {
            "before": [
                "leader",
                "f"
            ],
            "after": [
                "/"
            ]
        },
        // VSCode commands
        {
            "before": [
                "leader",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "leader",
                "."
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "leader",
                "o"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "enter"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        },
        {
            "before": [
                "leader",
                "b"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "leader",
                "m"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "leader",
                "t",
                "l"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "leader",
                "t",
                "h"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "r"
            ],
            "commands": [
                "workbench.action.debug.start"
            ]
        },
        // VSCode commands
        // Windows
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusSecondEditorGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusFirstEditorGroup"
            ]
        }
        // Windows
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        // Center screen
        {
            "before": [
                "leader",
                "z"
            ],
            "after": [
                "z",
                "z"
            ]
        },
        // Scroll down
        {
            "before": [
                "J"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "K"
            ],
            "after": [
                "<C-y>"
            ]
        },
    ],
    // "editor.fontFamily": "Red Hat Mono Medium", // https://fonts.google.com/specimen/Red+Hat+Mono
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "[html]": {
