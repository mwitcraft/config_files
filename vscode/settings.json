{
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "newUntitledFile",
    "vim.leader": " ",
    "vim.normalModeKeyBindingsNonRecursive": [
        // VIM remaps
        {
            "before": [
                "leader",
                "z"
            ],
            "after": [
                "z",
                "z"
            ]
        },
        // Scroll down
        {
            "before": [
                "J"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll down
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "K"
            ],
            "after": [
                "<C-y>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-y>"
            ]
        },
        {
            "before": [
                "leader",
                "f"
            ],
            "after": [
                "/"
            ]
        },
        // VSCode commands
        {
            "before": [
                "leader",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "leader",
                "."
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "leader",
                "o"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "enter"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        },
        {
            "before": [
                "leader",
                "b"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "leader",
                "m"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "leader",
                "t",
                "l"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "leader",
                "t",
                "h"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "r"
            ],
            "commands": [
                "workbench.action.debug.start"
            ]
        },
        // VSCode commands
        // Windows
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusSecondEditorGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusFirstEditorGroup"
            ]
        }
        // Windows
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        // Center screen
        {
            "before": [
                "leader",
                "z"
            ],
            "after": [
                "z",
                "z"
            ]
        },
        // Scroll down
        {
            "before": [
                "J"
            ],
            "after": [
                "<C-e>"
            ]
        },
        // Scroll up 
        {
            "before": [
                "K"
            ],
            "after": [
                "<C-y>"
            ]
        },
    ],
    "editor.fontFamily": "Fira Code",
    // "editor.fontFamily": "FiraMono Nerd Font",
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDragAndDrop": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "editor.fontSize": 16,
    "typescript.suggest.paths": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.formatOnType": true,
    "debug.console.wordWrap": false,
    "terminal.external.windowsExec": "C:\\Users\\WITCRAFM\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Nord",
    "workbench.editor.untitled.hint": "hidden",
    // "editor.renderWhitespace": "all",
    "editor.autoClosingDelete": "never",
    "editor.matchBrackets": "never",
    "terminal.integrated.fontFamily": "FiraMono Nerd Font",
    "terminal.integrated.fontSize": 16,
    // "editor.fontWeight": "400",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "redhat.telemetry.enabled": false,
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap sequence",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Join sequence",
        "!And sequence",
        "!If sequence",
        "!Not sequence",
        "!Equals sequence",
        "!Or sequence",
        "!FindInMap",
        "!Join",
        "!Sub sequence",
        "!ImportValue sequence",
        "!Split sequence"
    ],
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.guides.bracketPairs": "active",
    "explorer.copyRelativePathSeparator": "/",
    "http.proxySupport": "off",
    "zenMode.fullScreen": false,
    "zenMode.centerLayout": false,
    "zenMode.hideTabs": false,
    "zenMode.hideLineNumbers": false,
    "editor.lineNumbers": "relative",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.rulers": [
            88
        ],
        "editor.formatOnType": true,
    },
    "python.analysis.typeCheckingMode": "basic",
    "terminal.external.linuxExec": "\\mnt\\c\\Users\\WITCRAFM\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
    "vim.useCtrlKeys": false,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "icons",
    "security.workspace.trust.untrustedFiles": "open",
}