set ignorecase
let mapleader=" "

" Edit Functions
nnoremap <leader>d :vsc Edit.GoToDefinition<CR>
nnoremap <leader>i :vsc Edit.GoToImplementation<CR>
nnoremap <leader><leader>f :vsc Edit.FormatDocument<CR>
